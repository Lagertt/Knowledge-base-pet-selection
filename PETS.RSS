	MAINTEN=UNKNOWN
	COVER=UNKNOWN
	PET=UNKNOWN
	DIM VARR(5)
	USE VOPROS
	USE OTVETS

DO:
	clear
	if ((PET = "UNKNOWN") or (NOT (KNOWN("PET")))) then
		LOAD FORMOT2
		PUTFORM FORMOT2
		WAIT
		RELEASE FORMOT2
	else
		LOAD FORMOT1
		PUTFORM FORMOT1
		TALLY FORMOT1
		RELEASE FORMOT1
	endif
	release all	



RULE:	R1
	IF:	((MONBUY<3) AND (MONTIME<3)) OR ((MONBUY=3) AND (MONTIME=1))
	THEN:	BUDGET=1
	REASON:  
	
RULE:	R2
	IF:	((MONBUY>3) AND (MONTIME>2)) OR ((MONBUY=3) AND (MONTIME=4))
	THEN:	BUDGET=3
	REASON:    
	
RULE:	R3
	IF:	((MONBUY=3) AND ((MONTIME=2) OR (MONTIME=3))) OR (((MONBUY=5) AND (MONTIME=2)) OR ((MONBUY=2) AND (MONTIME=4))) OR ((MONBUY=4) AND (MONTIME<3)) OR ((MONBUY<3) AND (MONTIME=3))
	THEN:	BUDGET=2
	REASON:  
	
RULE:	R4
	IF:	(ALLER=1) OR (MESS=3)
	THEN:	COVER=3
	REASON:       -> 𐑒
	
RULE:	R5
	IF:	(ALLER=2) AND (MESS=2)
	THEN:	COVER=2
	REASON:        -> 𐑒
	
RULE:	R6
	IF:	(ALLER=2) AND (MESS=1)
	THEN:	COVER=1
	REASON:       ->    
	
RULE:	R7
	IF:	(WORK=2)
	THEN:	WORK12=2
	REASON:   ->    12 

RULE:	R8
