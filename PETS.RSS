GOAL:	PET

INITIAL:
	LOAD VXOD
	LOAD FORMOT1
	LOAD FORMOT2
	LOAD FORMV
	PUTFORM VXOD
	WAIT
	RELEASE VXOD
	CLEAR
	E.lstr=200
	E.TRYP="E"
	HOME=UNKNOWN
	YARD=UNKNOWN
	PASTURE=UNKNOWN
	PARK=UNKNOWN
	OUTSIDE=UNKNOWN
	MONBUY=UNKNOWN
	MONTIME=UNKNOWN
