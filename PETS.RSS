	REASON:    12  ->   
	
RULE:	R11
	IF:	(WORK12=1) AND (WORKSH=2)
	THEN:	FREETIME=2
	REASON:  12   ,    ->    
	
RULE:	R12
	IF:	(WORKSH=1) AND (ALONE=2)
	THEN:	FREETIME=1
	REASON:         ->   

RULE:	R13
	IF:	(HOME=2)	
	THEN:	PASTURE=2
	REASON:      ->    
	
RULE:	R14
	IF:	(HOME=2)
	THEN:	YARD=2
	REASON:      ->    
		
RULE:	R15
	IF:	((OUTSIDE=1) AND (YARD=2)) OR ((OUTSIDE=2) AND (PARK=2) AND (YARD=2))
	THEN:	WALK=1 
	REASON:         ->  
	
RULE:	R16
	IF:	((OUTSIDE=1) AND (YARD=1)) OR ((OUTSIDE=2) AND (PARK=1) AND (YARD=2)) OR ((OUTSIDE=3) AND (PARK=2) AND (YARD=2))
	THEN:	WALK=2 
	REASON:        ,   ,    ->  
	
RULE:	R17
	IF:	((OUTSIDE=3) AND (PARK=1) AND (YARD=2)) OR ((OUTSIDE=2) AND (YARD=1)) OR ((OUTSIDE=4) AND (PARK=2) AND (YARD=2))
	THEN:	WALK=3 
	REASON:        ,   ,    ->  - 
	
RULE:	R18
	IF:	((OUTSIDE=4) AND ((PARK=1) OR (YARD=1))) OR ((OUTSIDE=3) AND (YARD=1))
	THEN:	WALK=4 
	REASON:         ->  
	
RULE:	R19
	IF:	(FREETIME+WALK<4)
	THEN:	NTIME=1
	REASON:           ->     
	
RULE:	R20
	IF:	(FREETIME+WALK=4) OR (FREETIME+WALK=5)
	THEN:	NTIME=2
	REASON:     ,      ,   ->   -   
	
RULE:	R21
	IF:	 (FREETIME+WALK>5)
	THEN:	NTIME=3
	REASON:            ->     
	
RULE:	R22
	IF:	(CHILD=2)
	THEN:	SMALCHILD=2
	REASON:   ->   
	
RULE:	R23
	IF:	(SMALCHILD=1) OR (ANYPET=1)
	THEN:	CONTACT=2
	REASON:        ->  
	
RULE:	R24
	IF:	(SMALCHILD=2) AND (ANYPET=2)
	THEN:	CONTACT=1
	REASON:        ->    

RULE:	R25
	IF:	(NTIME=1)
	THEN:	MAINTEN=1
	REASON:     ->  
	
RULE:	R26
	IF: (NOT(NTIME=1) AND (CONTACT=1))
	THEN:	MAINTEN=3
	REASON:           ->   
	
RULE:	R27
	IF:	(NOT(NTIME=1) AND (CONTACT=2))
	THEN:	MAINTEN=2
	REASON:        ->     
	
	
	
	
	
RULE:	R28
	IF:	(BUDGET>0) AND (FARM=2) AND (COVER<4) AND (MAINTEN>0)
